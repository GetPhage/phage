<p id="notice"><%= notice %></p>

<h1>Cves</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Seq</th>
      <th>Status</th>
      <th>Desc</th>
      <th>Refs</th>
      <th>Comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cves.each do |cfe| %>
      <tr>
        <td><%= cfe.name %></td>
        <td><%= cfe.seq %></td>
        <td><%= cfe.status %></td>
        <td><%= cfe.desc %></td>
        <td><%= cfe.refs %></td>
        <td><%= cfe.comments %></td>
        <td><%= link_to 'Show', cfe %></td>
        <td><%= link_to 'Edit', edit_cfe_path(cfe) %></td>
        <td><%= link_to 'Destroy', cfe, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cfe', new_cfe_path %>
